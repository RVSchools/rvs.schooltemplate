 <transition transition_id="draft_internal" title="Owner drafts for Internal" new_state="draft_internal" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Draft - Internal</action>
  <guard>
   <guard-role>Owner</guard-role>
   <guard-role>Reviewer</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="draft_members" title="Owner drafts for Members" new_state="draft_members" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Draft - Members</action>
  <guard>
   <guard-role>Owner</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="draft_public" title="Owner drafts for Public" new_state="draft_public" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Draft - Public</action>
  <guard>
   <guard-role>Owner</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="publish_internal" title="Reviewer publishes for Internal" new_state="internal" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Publish - Internal</action>
  <guard>
   <guard-permission>Change portal events</guard-permission>
  </guard>
 </transition>
 <transition transition_id="publish_members" title="Reviewer publishes for Members" new_state="members" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Publish - Members</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <transition transition_id="publish_public" title="Reviewer publishes for Public" new_state="public" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Publish - Public</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <transition transition_id="reject_members" title="Reviewer rejects for Members" new_state="draft_members" trigger="USER" before_script="" after_script="">
  <action url="%(content_url)s/content_reject_form" category="workflow" icon="">Reject</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <transition transition_id="reject_public" title="Reviewer rejects for Public" new_state="draft_public" trigger="USER" before_script="" after_script="">
  <action url="%(content_url)s/content_reject_form" category="workflow" icon="">Reject</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <transition transition_id="retract" title="Owner retracts content" new_state="private" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Retract</action>
  <guard>
   <guard-role>Owner</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="retract_internal" title="Reviewer or Manager retracts" new_state="draft_internal" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Retract</action>
  <guard>
   <guard-role>Reviewer</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="retract_members" title="Owner retracts Member content" new_state="draft_members" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Retract</action>
  <guard>
   <guard-permission>Request review</guard-permission>
  </guard>
 </transition>
 <transition transition_id="retract_public" title="Owner retracts Public content" new_state="draft_public" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Retract</action>
  <guard>
   <guard-permission>Request review</guard-permission>
  </guard>
 </transition>
 <transition transition_id="review_members" title="Internal requests Member publish" new_state="pending_members" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Review - Members</action>
  <guard>
   <guard-role>Internal</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="review_public" title="Internal requests Public publish" new_state="pending_public" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Review - Public</action>
  <guard>
   <guard-role>Internal</guard-role>
   <guard-role>Manager</guard-role>
  </guard>
 </transition>
 <transition transition_id="unpublish_members" title="Reviewer/Manager pulls Member content offline" new_state="draft_members" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Unpublish</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <transition transition_id="unpublish_public" title="Reviewer/Manager pulls Public content offline" new_state="draft_public" trigger="USER" before_script="" after_script="">
  <action url="" category="workflow" icon="">Unpublish</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
 <worklist worklist_id="reviewer_member_queue" title="">
  <description>Reviewer Tasks</description>
  <action url="%(portal_url)s/search?review_state=pending_members" category="global" icon="">Pending members (%(count)d)</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
  <match name="review_state" values="pending_members"/>
 </worklist>
 <worklist worklist_id="reviewer_public_queue" title="">
  <description>Reviewer Tasks</description>
  <action url="%(portal_url)s/search?review_state=pending_public" category="global" icon="">Pending public (%(count)d)</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
  <match name="review_state" values="pending_public"/>
 </worklist>
 <variable variable_id="action" for_catalog="False" for_status="True" update_always="True">
  <description>The last transition</description>
  <default>
   
   <expression>transition/getId|nothing</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="actor" for_catalog="False" for_status="True" update_always="True">
  <description>The ID of the user who performed the last transition</description>
  <default>
   
   <expression>user/getId</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="comments" for_catalog="False" for_status="True" update_always="True">
  <description>Comments about the last transition</description>
  <default>
   
   <expression>python:state_change.kwargs.get('comment', '')</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="review_history" for_catalog="False" for_status="False" update_always="False">
  <description>Provides access to workflow history</description>
  <default>
   
   <expression>state_change/getHistory</expression>
  </default>
  <guard>
   <guard-permission>Request review</guard-permission>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </variable>
 <variable variable_id="time" for_catalog="False" for_status="True" update_always="True">
  <description>Time of the last transition</description>
  <default>
   
   <expression>state_change/getDateTime</expression>
  </default>
  <guard>
  </guard>
 </variable>
</dc-workflow>
